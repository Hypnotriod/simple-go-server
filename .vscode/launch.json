{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Debug",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/src",
            "env": {},
            "args": []
        },
        {
            "name": "Attach Debug",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}/src",
            "port": 2340,
            "host": "127.0.0.1"
        }
    ]
}